    "tokenColors": [
        {
            "name": "注释",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.fenced_code",
                "markup.inline"
            ],
            "settings": {
                "foreground": "#448666"
            }
        },
        {
            "name": "字符串、常数中的占位符",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#25aeee"
            }
        },
        {
            "name": "颜色",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#FF9999"
            }
        },
        {
            "name": "错误",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF3366"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "storage.control.clojure",
                "entity.name.function.clojure",
                "support.function.node",
            ],
            "settings": {
                "foreground": "#d73a49"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF9900"
            }
        },
        {
            "name": "关键词",
            "scope": [
                "keyword.control",
                "keyword.other.template",
                "keyword.other.substitution",
            ],
            "settings": {
                "foreground": "#FF6600"
            }
        },
        {
            "name": "标点符号, Misc",
            "scope": [
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
            ],
            "settings": {
                "foreground": "#ff8800bb"
            }
        },
        {
            "name": "标签",
            "scope": [
                "meta.tag",
                "meta.tag.sgml",
                "entity.name.tag",
                "markup.deleted.git_gutter",
                "string.interpolated",
                "string.template",
            ],
            "settings": {
                "foreground": "#FF8800"
            }
        },
        {
            "name": "函数名、特殊方法",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#E68444"
            }
        },
        {
            "name": "块级变量",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#f07178"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#f07178"
            }
        },
        {
            "name": "号码、常数、功能参数、标签属性、嵌入式",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
